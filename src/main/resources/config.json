{
	"models" : ["responseSingleSiteQualification","submitTrobuleTicket", "book","store","location"],
	
	"responseSingleSiteQualification":{
		"xpath":"//*[local-name()='ServiceQualificationComprisedOf']/ItemInvolvesLocation/type/text()",
		"value":"NBNLocationID",
		"getAsString":{
		"locationId":"//*[local-name()='ServiceQualificationComprisedOf']/ItemInvolvesLocation/Place/ID",
		"productSpecificationType":"//*[local-name()='ServiceQualificationComprisedOf']/ItemInvolvesProduct/SpecifiedBy/type"
		},
		
		"getInnerObjects":{
			"product":"product"		
		}
	},
	"product":{
		"getArrayofJsonObjects":{
			"productDetails":{ 
				"listXpath":"//*[local-name()='ServiceQualificationComprisedOf']/ItemInvolvesProduct/DescribedBy",
				"model":"productDetails"
			}
		}
	},
	
	"productDetails":{
		"getAsString":{
			"name":"Characteristic/name/text()",
			"type":"SpecifiedBy/type/text()",
			"value":"value/text()",
			"lan":"./@lang"
		}
		
	},
	"submitTrobuleTicket" : {
		"xpath": "//*[local-name()='IncidentTicket']/type/text()",
		"value": "Service Request",
		"getAsString": {
			"interactionId": "//*[local-name()='IncidentTicket']/AccessSeekerInteraction/ID/text()",
			"severity": "//*[local-name()='IncidentTicket']/severity/text()",
			"type": "//*[local-name()='IncidentTicket']/type/text()"
		}
	},
	
	"book": {
		"xpath": "name(/book)",
		"value": "book",
		"getAsString": {
			"root": "name(/book)",
			"title": "/book/title/text()",
			"lang": "/book/title/@lang"
		},
		"getAsInteger": {
			"year": "/book/year/text()"
		},
		"getAsDobule": {
			"price": "/book/price/text()"
		},
		"getNodeSet": {
			"authors": "/book/author/text()"
		},
		"getInnerObjects": {
			"Contributors": "contributor"
		}	
	},
	"contributor":{
			"getAsString":{
				"name":"//*[local-name()='contributor']/name/text()",
				"country":"//*[local-name()='contributor']/country/text()"
			}

	},
	
	"store":{
		"xpath": "name(/store)",
		"value": "store"
	},
	"location":{
		"xpath": "name(/location)",
		"value": "location"
	}
	
	
	

}